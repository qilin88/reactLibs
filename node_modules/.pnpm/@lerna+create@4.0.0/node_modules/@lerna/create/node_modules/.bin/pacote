#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/lib/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/pacote@11.3.5/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/pacote/lib/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/pacote/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/@lerna/create/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/@lerna/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/lib/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/pacote@11.3.5/node_modules/pacote/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/pacote@11.3.5/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/pacote/lib/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/pacote/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/@lerna/create/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+create@4.0.0/node_modules/@lerna/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../pacote/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../pacote/lib/bin.js" "$@"
fi
