#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/npm-packlist@2.2.2/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/npm-packlist/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/npm-packlist/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/@lerna/pack-directory/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/@lerna/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/npm-packlist@2.2.2/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/npm-packlist/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/npm-packlist/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/@lerna/pack-directory/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+pack-directory@4.0.0/node_modules/@lerna/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../npm-packlist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../npm-packlist/bin/index.js" "$@"
fi
