#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/installed-package-contents/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules/@npmcli/installed-package-contents/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules/@npmcli/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/installed-package-contents/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules/@npmcli/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@npmcli+installed-package-contents@1.0.7/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules/@npmcli/installed-package-contents/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules/@npmcli/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@npmcli/installed-package-contents/index.js" "$@"
else
  exec node  "$basedir/../@npmcli/installed-package-contents/index.js" "$@"
fi
