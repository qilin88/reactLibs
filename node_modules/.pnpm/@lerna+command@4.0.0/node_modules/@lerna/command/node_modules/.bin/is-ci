#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/is-ci@2.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules/is-ci/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules/@lerna/command/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules/@lerna/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/is-ci@2.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules/is-ci/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules/@lerna/command/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+command@4.0.0/node_modules/@lerna/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../is-ci/bin.js" "$@"
else
  exec node  "$basedir/../../../../is-ci/bin.js" "$@"
fi
