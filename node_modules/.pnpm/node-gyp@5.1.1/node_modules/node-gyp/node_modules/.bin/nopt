#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/nopt@4.0.3/node_modules/nopt/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/nopt@4.0.3/node_modules/nopt/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/nopt@4.0.3/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules/nopt/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules/nopt/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules/node-gyp/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/nopt@4.0.3/node_modules/nopt/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/nopt@4.0.3/node_modules/nopt/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/nopt@4.0.3/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules/nopt/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules/nopt/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node-gyp@5.1.1/node_modules/node-gyp/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../nopt/bin/nopt.js" "$@"
fi
