#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/rimraf@3.0.2/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules/rimraf/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules/@lerna/rimraf-dir/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules/@lerna/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/rimraf@3.0.2/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules/rimraf/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules/@lerna/rimraf-dir/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@lerna+rimraf-dir@4.0.0/node_modules/@lerna/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../rimraf/bin.js" "$@"
fi
