#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/lolcatjs/dist/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/lolcatjs/dist/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/lolcatjs/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/lolcatjs/dist/bin/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/lolcatjs/dist/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/lolcatjs/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules/@darkobits/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/@darkobits+lolcatjs@3.1.5/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../../dist/bin/cli.js" "$@"
fi
