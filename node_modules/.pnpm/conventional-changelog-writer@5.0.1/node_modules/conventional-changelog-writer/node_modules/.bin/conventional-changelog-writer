#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules/conventional-changelog-writer/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules/conventional-changelog-writer/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/conventional-changelog-writer@5.0.1/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/lib/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
