#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.72.0/node_modules/webpack-cli/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.72.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.72.0/node_modules/webpack-cli/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.72.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.1.0/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.1.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
