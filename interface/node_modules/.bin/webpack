#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack@5.72.0_1a3da91f68f2edae254d0c8f1249b213/node_modules/webpack/bin/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack@5.72.0_1a3da91f68f2edae254d0c8f1249b213/node_modules/webpack/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack@5.72.0_1a3da91f68f2edae254d0c8f1249b213/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/webpack/bin/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/webpack/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack@5.72.0_1a3da91f68f2edae254d0c8f1249b213/node_modules/webpack/bin/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack@5.72.0_1a3da91f68f2edae254d0c8f1249b213/node_modules/webpack/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/webpack@5.72.0_1a3da91f68f2edae254d0c8f1249b213/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/.pnpm/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules:/Users/chen/learn/project2021/reactLib/node_modules:/Users/chen/learn/project2021/node_modules:/Users/chen/learn/node_modules:/Users/chen/node_modules:/Users/node_modules:/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/webpack/bin/node_modules:/Users/chen/learn/project2021/reactLib/interface/node_modules/webpack/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
